# Development
NODE_ENV=development

# Instrumentation
# Enable arrange session probe overlay (set to 1 during Flow rehearsals only)
VITE_SESSION_PROBE=0
# Allow exporting session probe data as JSON (set to 1 to enable export button)
VITE_SESSION_PROBE_ALLOW_EXPORT=0